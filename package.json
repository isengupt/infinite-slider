    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "gsap": "^3.5.1",
    "math-fit": "^1.0.1",
    "pixi.js": "^5.3.4"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "gh-pages": "^3.1.0",
    "parcel": "^1.12.4"
  }
}
